;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.digicore-14.test. usuari.digicore-14.test (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; name server NS records
	  	 IN	NS	ns1.digicore-14.test.

; registers
ns1	 	 IN	A	192.168.4.14
server	 IN	A	192.168.4.14
dbserver IN	A	192.168.4.114
data	 IN	CNAME	dbserver.digicore-14.test.
